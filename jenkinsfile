pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                    git branch: 'main', url: 'git@github.com:Teja-Aws/Sample.git''
                  }
            
            
       }
 stage('ci') {
            steps {
                echo 'Hello World'
                  }
            
            
       }
 stage('cd') {
            steps {
                echo 'Hello World'
                  }
            
            
       }
    }
}
