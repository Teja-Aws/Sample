pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                    git clone https://github.com/Teja-Aws/Sample.git /var/www/html
                  }
            
            
       }
 stage('ci') {
            steps {
                echo 'Hello World'
                  }
            
            
       }
 stage('cd') {
            steps {
                echo 'Hello World'
                  }
            
            
       }
    }
}
