    agent any 
    stages {
        stage('Build') { 
            steps {
              git branch: 'main', credentialsId: 'c32bcd78-8e80-4992-aec2-47abf50f1179', url: 'https://github.com/Teja-Aws/Sample.git'
                  }
        }
        stage('Test') { 
            steps {
                echo("Hello World")
            }
        }
        stage('Deploy') { 
            steps {
                sh 'cp -R * /var/www/html/'
            }
        }
    }
}
